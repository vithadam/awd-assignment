<!DOCTYPE html>
<html>
<head>
  
  <title>Eight Practical</title>
  <script>
    // Creating array
    var arr = [1,2,3,4,5]
  
    // Creating variable to store the sum
    var sum = 0;
  
    // Running the for loop
    for (let i = 0; i < arr.length; i++) {
      sum += arr[i];
    }
  
    document.write("Sum is " + sum) // Prints: 44
  </script>
  </head>

  <body>
  </body>
  </html>
